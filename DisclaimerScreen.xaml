<UserControl x:Class="Iap.DisclaimerScreen"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <Grid Width="1746" Height="738">
        <Grid.Background>
            <ImageBrush ImageSource="/Images/disclaimerscreen.png"/>
        </Grid.Background>
        <Button x:Name="CloseDisclaimer" Margin="1690,10,0,0"  HorizontalAlignment="Left"
                VerticalAlignment="Top" Width="46" Height="65" Click="CloseDisclaimer_Click">
        </Button>
        <Image Source="/Images/AIA_FOR PNG-12.png" Margin="1690,10,10,663" Width="17" Height="19" IsHitTestVisible="False"/>
    </Grid>
</UserControl>
